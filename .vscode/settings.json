{
    "terminal.integrated.fontSize": 12,
    "editor.accessibilitySupport": "off",
    "window.zoomLevel": 0,
    "workbench.tree.indent": 15,
    "explorer.compactFolders": false,
    "workbench.colorCustomizations": {
        "tree.indentGuidesStroke": "#3cff00"
    },
    "clang-format.executable": "clang-format",
    "clang-format.style": "file",
    "clang-format.fallbackStyle": "Google",
    "clang-format.formatOnSave": true,
    "clang-format.formatOnType": true,
    "clang-format.arguments": [
        "-assume-filename=${workspaceFolder}/.clang-format"
    ],
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": false,
        "markdown": false,
        "python": true
    },
    "editor.fontSize": 12,
    "[python]": {
        "editor.formatOnType": true
    },
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Default Dark Modern",
    "editor.fontFamily": "'Cascadia Code', Monaco, Consolas, Monaco, 'Courier New', monospace",
    "files.autoSave": "afterDelay",
    "git.openRepositoryInParentFolders": "never",
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "python.autoComplete.extraPaths": [
        "/opt/ros/foxy/lib/python3.8/site-packages",
        "/opt/carla-simulator/PythonAPI/carla/dist/carla-0.9.14-py3.8-linux-x86_64.egg",
        "/opt/carla-simulator/PythonAPI/carla"
    ],
    "python.analysis.extraPaths": [
        "/opt/ros/foxy/lib/python3.8/site-packages",
        "/opt/carla-simulator/PythonAPI/carla/dist/carla-0.9.14-py3.8-linux-x86_64.egg",
        "/opt/carla-simulator/PythonAPI/carla"
    ],
    "C_Cpp.errorSquiggles": "disabled",
    "files.associations": {
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "csignal": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
